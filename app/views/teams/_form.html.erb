<%= simple_form_for(@team, html: {class: 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, 		label: "Name" %>
    <%= f.input :city, 		label: "City" %>
    <%= f.input :state, 	label: "State" %>
    <%= f.button :submit %>
  </div>
<% end %>
