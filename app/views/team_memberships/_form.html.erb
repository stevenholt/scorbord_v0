<%= simple_form_for(@team_membership) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :team_id, :as => :hidden, input_html: { value: @team.id } %>
    <%= f.input :person_id %>
    <%= f.input :jersey_num %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
